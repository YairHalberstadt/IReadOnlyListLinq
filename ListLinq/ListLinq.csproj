<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>latest</LangVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Yair Halberstadt</Authors>
    <Company>YLib</Company>
    <Description>A Linq provider for IReadOnlyList</Description>
    <Copyright>MIT</Copyright>
    <PackageLicenseUrl>https://github.com/YairHalberstadt/ListLinq/blob/master/LICENSE</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/YairHalberstadt/ListLinq/tree/master/ListLinq</PackageProjectUrl>
    <PackageIconUrl>https://github.com/YairHalberstadt/ListLinq/blob/master/ListLinq/icon.ico</PackageIconUrl>
    <RepositoryUrl>https://github.com/YairHalberstadt/ListLinq</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>Linq</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="InvalidStateException" Version="0.0.2" />
  </ItemGroup>

</Project>
